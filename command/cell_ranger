# Cell Ranger 실행 기록

## Pbmc_1K (GRCh38)
nohup /home/r22100180/project/cellranger-9.0.1/cellranger count \
  --id=sample_GRCh38 \
  --transcriptome=/home/r22100180/project/refdata-gex-GRCh38-2024-A \
  --fastqs=/home/r22100180/sample/pbmc_1k_v3_fastqs \
  --sample=pbmc_1k_v3 \
  --chemistry=auto \
  --create-bam=true \
  --localcores=8 \
  --localmem=64 > cellranger.log 2>&1 &

## Pbmc_10K 
nohup /home/r22100180/project/cellranger-9.0.1/cellranger count \
  --id=pbmc_10k \
  --transcriptome=/home/r22100180/project/refdata-gex-GRCh38-2024-A \
  --fastqs=/home/r22100180/data/pbmc_10k_v3/pbmc_10k_v3_fastqs \
  --sample=pbmc_10k_v3 \
  --chemistry=auto \
  --create-bam=true \
